<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/first"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:paddingVertical="10dp">
        <androidx.cardview.widget.CardView
            android:id="@+id/RazRab"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHeight_percent="0.8"
            app:layout_constraintWidth_percent="0.2"
            app:cardCornerRadius="50dp">

            <ImageView
                android:id="@+id/Danil_image"
                android:layout_width="120dp"
                android:layout_height="110dp"
                android:layout_gravity="center"
                android:layout_centerVertical="true"
                android:src="@drawable/donil"
                android:transitionName="@string/trans" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/Danil"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="Куртымов Данил"
            android:fontFamily="@font/semibold"
            android:textColor="#000000"
            android:textSize="17sp"
            android:gravity="bottom"
            app:layout_constraintStart_toEndOf="@+id/RazRab"
            android:layout_marginStart="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHeight_percent="0.4"
            app:layout_constraintWidth_percent="0.6"
            />
        <TextView
            android:id="@+id/Danil_info"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="Дизайнер"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/semibold"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/RazRab"
            app:layout_constraintTop_toBottomOf="@id/Danil"
            app:layout_constraintHeight_percent="0.4"
            app:layout_constraintWidth_percent="0.6"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/second"
        android:layout_below="@+id/first"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:paddingVertical="10dp">
        <androidx.cardview.widget.CardView
            android:id="@+id/RazRab2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHeight_percent="0.8"
            app:layout_constraintWidth_percent="0.2"
            app:cardCornerRadius="50dp">

            <ImageView
                android:id="@+id/Rustem_image"
                android:layout_width="120dp"
                android:layout_height="110dp"
                android:layout_gravity="center_horizontal"
                android:layout_centerVertical="true"
                android:src="@drawable/rustem"
                android:transitionName="@string/trans" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/Rustem"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:text="Хамзин Рустем"
            android:fontFamily="@font/semibold"
            android:textColor="#000000"
            android:textSize="17sp"
            android:gravity="bottom"
            android:layout_marginEnd="20dp"
            app:layout_constraintEnd_toStartOf="@+id/RazRab2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHeight_percent="0.4"
            />
        <TextView
            android:id="@+id/Rustem_info"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:text="Front-end разработчик"
            android:layout_marginEnd="20dp"
            android:fontFamily="@font/semibold"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/RazRab2"
            app:layout_constraintTop_toBottomOf="@id/Rustem"
            app:layout_constraintWidth_percent="0.6"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/third"
        android:layout_below="@id/second"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:paddingVertical="10dp">
        <androidx.cardview.widget.CardView
            android:id="@+id/RazRab3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHeight_percent="0.8"
            app:layout_constraintWidth_percent="0.2"
            app:cardCornerRadius="50dp">

            <ImageView
                android:id="@+id/Kamil_image"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:src="@drawable/kamil"
                android:transitionName="@string/trans" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/Kamil"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="Рязапов Камиль"
            android:fontFamily="@font/semibold"
            android:textColor="#000000"
            android:textSize="17sp"
            android:gravity="bottom"
            app:layout_constraintStart_toEndOf="@+id/RazRab3"
            android:layout_marginStart="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHeight_percent="0.4"
            app:layout_constraintWidth_percent="0.6"
            />
        <TextView
            android:id="@+id/Kamil_info"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="Оператор базы данных"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/semibold"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/RazRab3"
            app:layout_constraintTop_toBottomOf="@id/Kamil"
            app:layout_constraintHeight_percent="0.4"
            app:layout_constraintWidth_percent="0.6"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/fourth"
        android:layout_below="@+id/third"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:paddingVertical="10dp">
        <androidx.cardview.widget.CardView
            android:id="@+id/RazRab4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHeight_percent="0.8"
            app:layout_constraintWidth_percent="0.2"
            app:cardCornerRadius="50dp">

            <ImageView
                android:id="@+id/Samat_image"
                android:layout_width="120dp"
                android:layout_height="110dp"
                android:layout_gravity="center_horizontal"
                android:src="@drawable/samat"
                android:transitionName="@string/trans" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/Samat"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:text="Аскаров Самат"
            android:fontFamily="@font/semibold"
            android:textColor="#000000"
            android:textSize="17sp"
            android:gravity="bottom"
            android:layout_marginEnd="20dp"
            app:layout_constraintEnd_toStartOf="@+id/RazRab4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHeight_percent="0.4"
            />
        <TextView
            android:id="@+id/Samat_info"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:text="Back-end разработчик"
            android:layout_marginEnd="20dp"
            android:fontFamily="@font/semibold"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/RazRab4"
            app:layout_constraintTop_toBottomOf="@id/Samat"
            app:layout_constraintWidth_percent="0.6"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
        </RelativeLayout>
</RelativeLayout>